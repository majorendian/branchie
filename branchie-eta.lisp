(defun clone-map (listmap)
  (map 'list #'copy-seq listmap))

(defmethod WASD-keys ((key string) (pp player-pos))
  (let ((new-pos (make-instance 'player-pos
                                 :x (player-pos-x pp)
                                 :y (player-pos-y pp))))
   (cond
     ((string= key "W") (setf (player-pos-y new-pos) (- (player-pos-y new-pos) 1)))
     ((string= key "A") (setf (player-pos-x new-pos) (- (player-pos-x new-pos) 1)))
     ((string= key "S") (setf (player-pos-y new-pos) (+ 1 (player-pos-y new-pos))))
     ((string= key "D") (setf (player-pos-x new-pos) (+ 1 (player-pos-x new-pos)))))
   new-pos))

(defmethod process-map ((cur_map list))
  (for-each-column (lambda (x y mapcopy))
                   cur_map))

(defmethod process-entity-map ((e_map list) (pp player-pos))
  (for-each-column (lambda (x y mapcopy)
                     (let ((tile (nth x (nth y mapcopy))))
                       (cond
                         ((= tile 1) (progn
                                       ;found player tile
                                       (setf (player-pos-x pp) x)
                                       (setf (player-pos-y pp) y)))
                         )))
                   e_map))

(defmethod merge-maps ((map1 list) (map2 list))
  (loop for row in map1
        for y = 0 then (+ 1 y)
        collect (loop for col in row
                      for x = 0 then (+ 1 x)
                      collect (progn
                                (if (not (= col 0))
                                  col
                                  (nth x (nth y map2)))))))

(defun eta-loop (active_branch &key
                               (static-map *current-map*)
                               (entity-map *entity-map*)
                               (on-pre-init (lambda ()))
                               (on-post-init (lambda ()))
                               (on-update nil)
                               (on-key (lambda (key) (declare (ignore key))))
                               (on-branch-jump (lambda (b) (declare (ignore b))))
                               )
  (setf branchie-ta:*process-input-key* nil)
  (init-translation-table)
  (let ((pp (make-instance 'player-pos)))
    (ta-loop 960 560 active_branch
             :on-post-init (lambda ()
                             (funcall on-pre-init)
                             (init-elements)
                             (funcall on-post-init)
                             (process-entity-map static-map pp)
                             (draw-map (merge-maps entity-map static-map)))
             :on-update on-update
             :on-branch-jump on-branch-jump
             :on-key (lambda (key)
                       (let* ((new-pos (WASD-keys key pp))
                              (player_map (new-map)))
                         (if (and (< -1 (player-pos-x new-pos))
                                  (< -1 (player-pos-y new-pos))
                                  (> (length (nth 0 player_map)) (player-pos-x new-pos))
                                  (> (length player_map) (player-pos-y new-pos)))
                           (progn
                             (setf (nth (player-pos-x new-pos) (nth (player-pos-y new-pos) player_map)) 1)
                             (setf pp new-pos))
                           (progn
                             (setf (nth (player-pos-x pp) (nth (player-pos-y pp) player_map)) 1)
                             ))
                         (draw-map (merge-maps player_map static-map))))
             :text-speed 20)))
