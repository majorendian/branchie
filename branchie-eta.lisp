
(defpackage :branchie-eta
  (:use :cl :branchie-ta :branchie-tiny-ui :branchie-core :ltk)
  (:export eta-loop
           get-image-canvas))

(in-package :branchie-eta)

(defclass player-pos ()
  ((x :accessor player-pos-x :initarg :x :initform 0)
   (y :accessor player-pos-y :initarg :y :initform 0)))

(defun init-elements ()
  (configure (textbox (get-textarea))
             :width (floor (/ (parse-integer (cget (get-canvas) :width)) 12.5))
             :height (floor (/ (parse-integer (cget (get-canvas) :height)) 50)))
  (let ((image-canvas (make-instance 'canvas
                                     :background "black"
                                     :master (get-canvas)
                                     :width (floor (/ (parse-integer (cget (get-canvas) :width)) 1.57))
                                     :height (floor (/ (parse-integer (cget (get-canvas) :height)) 2.2)) )))
    (format-wish "~a create window ~a ~a -window ~a -anchor nw -tags image-canvas"
                 (widget-path (get-canvas))
                 (floor (/ (parse-integer (cget (get-canvas) :width)) 25))
                 (floor (/ (parse-integer (cget (get-canvas) :height)) 2.3))
                 (widget-path image-canvas))
    ;"export" the image canvas so we can access it from outside this function
    (defun get-image-canvas () image-canvas)))

(defparameter *column-translation-table* (make-hash-table))

(defun init-translation-table ()
  (setf (gethash 0 *column-translation-table*) ".")
  (setf (gethash 1 *column-translation-table*) "@")
  (setf (gethash 7 *column-translation-table*) "#"))

(defmethod translate-mapcolumn ((col fixnum))
  (gethash col *column-translation-table*))

(defun for-each-column (func listmap)
  (let ((mapcopy (clone-map listmap)))
    (loop for row in mapcopy
          for y = 0 then (+ y 1)
          do (loop for col in row
                   for x = 0 then (+ x 1)
                   do (progn
                        (funcall func x y mapcopy))))
    mapcopy))

(defun draw-map (listmap &key process-func)
  (let ((buffer ""))
    (loop for row in listmap
          for y = 0 then (+ 1 y)
          do (progn
               (loop for col in row
                     for x = 0 then (+ 1 x)
                     do (progn
                          (when process-func
                            (funcall process-func x y))
                          (setf buffer (concatenate 'string buffer (translate-mapcolumn col)))
                          ))
               (setf buffer (format nil "~a~%" buffer))))
    (format-wish "~a delete map" (widget-path (get-image-canvas)))
    (format-wish "set _text_map ~S~%
                  ~a create text 0 0 -fill white -anchor nw -font \"Monospace 11\" -tags map -text $_text_map"
                 buffer
                 (widget-path (get-image-canvas)))))

